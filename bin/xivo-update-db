#!/usr/bin/env python
# -*- coding: UTF-8 -*-

import os
import subprocess

OLD_BIN_DIR = '/usr/lib/xivo-manage-db/old'


def main():
    filename = os.path.join(OLD_BIN_DIR, 'xivo-update-db-old')
    args = [filename]
    popen = subprocess.Popen(args, stdout=subprocess.PIPE)
    popen.wait()
    output = popen.stdout.read()
    print output

main()
